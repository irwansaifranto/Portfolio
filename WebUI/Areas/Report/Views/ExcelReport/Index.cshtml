@using WebUI.Extension
@model WebUI.Areas.Report.Models.ExcelReportFilterModel
@{
    ViewBag.Title = "Download Excel";
    //Layout = "~/Views/Shared/_Layout.cshtml";
    var currentController = (WebUI.Areas.Report.Controllers.ExcelReportController)ViewContext.Controller;
}
<div class="page-header">
    <h1>@ViewBag.Title</h1>
</div>

@Html.Raw(currentController.GetMessage())

<div class="form-horizontal">
    <div class="form-group">
        <label class="control-label col-xs-2">Data Didownload</label>
        <div class="col-xs-4">
            <select class="form-control">
                <option>Booking</option>
            </select>
        </div>
        @Html.ValidationMessageFor(model => model.ReportType)
    </div>
    <div class="form-group">
        <div class="col-xs-offset-2 col-xs-4">
            @*<button id="btnFilter" type="submit" class="btn btn-default">Download</button>*@
            @Html.ActionLink("Index", "Excelreport", new { area = "" }, new { @class = "btn btn-primary" })
        </div>
    </div>
</div>

